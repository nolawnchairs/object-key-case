{
  "name": "@nolawnchairs/object-key-case",
  "version": "0.0.1",
  "description": "Utility for converting object keys to camelCase or snake_case",
  "keywords": [
    "object",
    "key",
    "case",
    "camel case",
    "snake case",
    "case conversion",
    "transform"
  ],
  "homepage": "https://github.com/nolawnchairs/object-key-case#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nolawnchairs/object-key-case.git"
  },
  "bugs": {
    "url": "https://github.com/nolawnchairs/object-key-case/issues"
  },
  "author": "Michael Wieczorek",
  "license": "MIT",
  "main": "lib/index.js",
  "access": "public",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "../src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "jest": "^29.5.0",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "rimraf lib && tsc",
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  }
}